# basic folder shortcut aliases

alias mus='pushd ~/Music'
alias pic='pushd ~/Pictues'
alias vid='pushd ~/Videos'
alias dow='pushd ~/Downloads'
alias doc='pushd ~/Documents'

# basic git shortcut aliases

alias g='git'
alias gi='git init'
alias gs='git status'
alias ga='git add'
alias gA='git add .'
alias gc='git commit -m'
alias gt='git tag'
alias gp='git push'
alias gP='git pull'
alias gS='git stash'

# basic utilities

alias one='onefetch'
alias ch='clear && histoy -c'
alias cls='clear'
alias m='man'
alias ?='--help'

